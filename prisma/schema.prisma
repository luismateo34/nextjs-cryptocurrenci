generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_DATABASE_URL")
}

model Listnamecripto {
  id     String @id
  symbol String
  name   String
}

model DerivateCripto {
  market                      String
  symbol                      String
  index_id                    String  @id
  price                       String
  price_percentage_change_24h Float
  contract_type               String
  index                       Int?
  basis                       Float
  spread                      Float?
  funding_rate                Float
  open_interest               Float?
  volume_24h                  Float
  last_traded_at              Int
  expired_at                  Int?
}
